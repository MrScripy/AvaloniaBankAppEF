<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="AvaloniaBankAppEF.App"
             RequestedThemeVariant="Default">

	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/Assets/Styles/ColorsDictionary.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
    <Application.Styles>		
        <FluentTheme />
		<StyleInclude Source="avares://Avalonia.Controls.DataGrid/Themes/Fluent.xaml"/>
		<StyleInclude Source="/Assets/Styles/BtnStyles.axaml"/>
		<StyleInclude Source="/Assets/Styles/TextStyles.axaml"/>
		<StyleInclude Source="/Assets/Styles/DGStyles.axaml"/>
    </Application.Styles>
</Application>
