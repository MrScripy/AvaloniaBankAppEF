<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:views="clr-namespace:AvaloniaBankAppEF.Views.Dialogs.Pages;assembly=AvaloniaBankAppEF"
        mc:Ignorable="d" d:DesignWidth="500" d:DesignHeight="500"
		Width="500" Height="500"
		CanResize="False"
        x:Class="AvaloniaBankAppEF.Views.Dialogs.DialogWindow"
        Title="DialogWindow">
	
	<Window.Background>
		<LinearGradientBrush StartPoint="0%, 0%" EndPoint="0%, 100%">
			<GradientStop Color="{StaticResource WhiteBrush}" Offset="0"/>
			<GradientStop Color="{StaticResource BlueBrush}" Offset="1"/>
		</LinearGradientBrush>
	</Window.Background>
	
	<views:AddCustomer/>
	
</Window>
